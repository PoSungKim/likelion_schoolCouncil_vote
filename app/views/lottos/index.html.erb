<p id="notice"><%= notice %></p>

<h1>Lottos</h1>

<table>
  <thead>
    <tr>
      <th>Totalnum</th>
      <th>Winnum</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lottos.each do |lotto| %>
      <tr>
        <td><%= lotto.totalnum %></td>
        <td><%= lotto.winnum %></td>
        <td><%= lotto.result %></td>
        <td><%= link_to 'Show', lotto %></td>
        <td><%= link_to 'Edit', edit_lotto_path(lotto) %></td>
        <td><%= link_to 'Destroy', lotto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lotto', new_lotto_path %>
